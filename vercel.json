{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,

  "functions": {
    "api/index.py": {
      "runtime": "dockerfile"
    }
  },

  "routes": [
    {
      "src": "/extract",
      "methods": ["POST"],
      "dest": "api/index.py"
    },
    {
      "src": "/test",
      "methods": ["GET"],
      "dest": "api/index.py"
    }
  ]
}
